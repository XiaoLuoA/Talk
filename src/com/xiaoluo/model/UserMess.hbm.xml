<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	<class name="com.xiaoluo.model.UserMess" table="user_mess">
		<id name="userMessId" column="user_mess_id">
			<generator class="identity"/>
		</id>
		<property name="itemId" column="item_id"/>
		<property name="fromId" column="from_id"/>
		<property name="sendTime" column="send_time"/>
		<property name="toId" column="to_id"/>
		<property name="isRead"/>
		<property name="content" />
	</class>
</hibernate-mapping>
